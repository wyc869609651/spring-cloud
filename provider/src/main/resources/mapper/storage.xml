<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wyc.provider.mapper.StorageMapper">
  <update id="storageDown">
        update `seata_storage` set `count` = `count` - #{count} where `commodity_code` = #{commodityCode}
  </update>
</mapper>