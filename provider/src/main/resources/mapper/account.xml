<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wyc.provider.mapper.AccountMapper">
  <update id="detach">
    update `seata_account` set `account` = `account` - #{money} where `user` = #{user}
  </update>
</mapper>